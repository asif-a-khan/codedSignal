{
  "expo": {
    "name": "coded-signal",
    "slug": "coded-signal",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/signal.png",
    "splash": {
      "image": "./assets/signal-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#2C6BED"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.codedkhan.coded-signal"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "A messaging app inspired by Signal",
    "githubUrl": "https://github.com/asif-a-khan/codedSignal"
  }
}
